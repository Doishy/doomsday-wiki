{
  "name": "doomsday-wiki",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "material-ui": "^1.0.0-beta.37",
    "mdi-material-ui": "^3.1.0",
    "node-sass-chokidar": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "typeface-roboto": "0.0.54"
  },
  "scripts": {
    "build": "npm run build-css && react-scripts build",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p watch-css watch-js",
    "test": "react-scripts test --env=jsdom",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/ -o src/styles/ --watch",
    "watch-js": "react-scripts start",
    "build-css": "node-sass-chokidar src/styles/ -o src/styles/"
  }
}
